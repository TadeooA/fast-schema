{
  "name": "@tadeoa/fast-schema",
  "version": "1.0.0",
  "description": "The fastest TypeScript-first schema validation with static type inference - a drop-in replacement for Zod with 10-20x better performance",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "pkg",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:ts",
    "build:wasm": "wasm-pack build --target bundler --out-dir js/pkg",
    "build:ts": "tsc && rollup -c",
    "dev": "npm run build:wasm && tsc --watch",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "benchmark": "node benchmarks/vs-zod.js",
    "benchmark:all": "node benchmarks/comprehensive.js",
    "examples": "npm run examples:basic && npm run examples:advanced && npm run examples:html",
    "examples:basic": "node examples/basic-usage/simple-validation.js",
    "examples:advanced": "node examples/advanced-usage/complex-schemas.js",
    "examples:html": "node examples/html-validation/basic-elements.js",
    "examples:migration": "node examples/migration-guide/from-zod.js",
    "lint": "eslint js/src --ext .ts",
    "format": "prettier --write 'js/src/**/*.ts'",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "schema",
    "validation",
    "typescript",
    "zod",
    "performance",
    "wasm",
    "rust",
    "type-safe",
    "json-schema",
    "parsing",
    "runtime-validation",
    "static-typing",
    "drop-in-replacement"
  ],
  "author": "Fast-Schema Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TadeooA/fast-schema.git"
  },
  "bugs": {
    "url": "https://github.com/TadeooA/fast-schema/issues"
  },
  "homepage": "https://github.com/TadeooA/fast-schema#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "rollup": "^3.20.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}